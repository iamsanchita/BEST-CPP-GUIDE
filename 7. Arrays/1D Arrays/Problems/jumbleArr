#include <stdio.h>

void Sort(int *arr, int size) {
    for (int i = 0; i < size - 1; i++) {
        for (int j = 0; j < size - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}

void equal(int *arr, int *brr, int size) {
    int flag = 0; 
    for (int k = 0; k < size; k++) {
        if (arr[k] != brr[k]) {
            flag = 1;
            printf("not same\n");
            break;
        }
    }
    if (flag == 0) {
        printf("same\n");
    }
}

int main() {
    int brr[] = {1, 3, 2, 5, 4};
    int arr[] = {5, 3, 2, 2, 4};
    int size = sizeof(arr) / sizeof(int);
    Sort(arr, size);
    Sort(brr, size);
    equal(arr, brr, size);
    return 0;
}
